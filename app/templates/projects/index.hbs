<div id="template-projects-index">
  <h1>Portfolio</h1>

  <ul>
  {{#each models}}
    <li>
    	<a href="/projects/{{id}}">
    		<img src="/images/projects/{{id}}.png" />
    		{{name}}

    	</a>
    	<div class="tags">
  		{{#each tags}}{{#if @index}},{{/if}}
    			<a href="/projects/tags/{{this}}">{{this}}</a>{{/each}}
  	  </div>
    </li>
  {{/each}}
  </ul>
</div>